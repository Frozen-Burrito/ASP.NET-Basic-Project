@page
@model ListModel
@{
}

<h1>Restaurants</h1>

<table class="table">
    <th>Id</th> 
    <th>Name</th>
    <th>Location</th>
    <th>Cuisine Type</th>
    
    @foreach(var restaurant in Model.Restaurants)
    {
        <tr>
            <td>@restaurant.Id</td>
            <td>@restaurant.Name</td>
            <td>@restaurant.Location</td>
            <td>@restaurant.Cuisine</td>
        </tr>
    }
</table>

<div>
    @Model.Message
</div>